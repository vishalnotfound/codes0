class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        int n = nums.size();

        k = k%n; //ensure ki repeat na ho baar baar and like if k = 12, n= 5 then it just do 2 shifts insted of doint 5+5+2;

        reverse(nums.begin(), nums.end());
        reverse(nums.begin(), nums.begin()+k);
        reverse(nums.begin()+k, nums.end());
    }
};
